<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hotelmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManagerActivity" />
        <activity android:name=".ListItemsActivity" />
        <activity android:name=".PlaceOrder" />
        <activity android:name=".ConfirmedActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".UserRegister" />
        <activity android:name=".ManagerRegister" />
        <activity android:name=".ChefRegistration" />
        <activity android:name=".ManagerMenu" />
        <activity android:name=".OrdersList" />
        <activity android:name=".ChefMenu" />
        <activity android:name=".UsersMenu" />
        <activity android:name=".OrderStatusActivity"></activity>
    </application>

</manifest>